---
const { type, width, height, vb, d } = Astro.props;
---
{
    type === 'svg' &&
        <?xml version="1.0" encoding="UTF-8" standalone="no"?>
        <svg
            width={width}
            height={height}
            viewBox={vb}
            version="1.1"
                xmlns="http://www.w3.org/2000/svg"
                xmlns:svg="http://www.w3.org/2000/svg"
                aria-hidden="true"
                >
                <g>
                    <path
                        d={d}
                    />
                </g>
            </svg>
}