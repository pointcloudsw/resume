---
import { getEntry } from 'astro:content';
const { svg } = Astro.props;
const svgImg = await getEntry('svg',svg);
---
<?xml version="1.0" encoding="UTF-8" standalone="no"?>
    <svg
        width={svgImg?.data?.width}
        height={svgImg?.data?.height}
        viewBox={svgImg?.data?.viewBox}
        class={svgImg?.data?.class}
        version="1.1"
        xmlns="http://www.w3.org/2000/svg"
        aria-hidden="true"
    >
        <path
            d={svgImg?.data?.d}
        />
    </svg>